    "postbuild": "npm run doc",
    "doc": "npx typedoc src/index.ts --json doc/doc.json --out doc",
    "format": "prettier --write ./src/**/*.ts"
  },
  "dependencies": {
    "@serverless-devs/core": "^0.0.*",
    "hasha": "^5.2.2",
    "i18n": "^0.13.2",
    "js-yaml": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lodash.get": "^4.4.2",
    "mime": "^2.5.2",
    "node-fetch": "^2.6.1",
    "tty-table": "^4.1.3"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/i18n": "^0.13.1",
    "@types/js-yaml": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.172",
    "@types/lodash.get": "^4.4.6",
    "@types/mime": "^2.0.3",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.12",
    "@types/prettier": "^2.3.2",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "prettier": "^2.3.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.7"
  }
